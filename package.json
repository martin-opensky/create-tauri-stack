{
  "name": "embrace-community",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn dev:api\" \"yarn dev:fe\"",
    "dev:fe": "vite",
    "dev:api": "cd apps/api && yarn dev",
    "dev:ios": "yarn tauri ios dev --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\""
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@tanstack/react-router": "^1.81.4",
    "@tanstack/router-devtools": "^1.81.4",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "graphql": "^16.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  },
  "workspaces": [
    "apps/*"
  ]
}
